{% for idx, form_page in form %}
<table>
{{ form_page|raw }}
</table>
<form action="?P=i{{idx+1}}" method="post">
	<p><button type="submit">修正する</button></p>
	{{ hidden|raw }}
</form>
{% endfor %}

<form action="?P=e" method="post">
	<p><button type="submit">実行する</button></p>
	{{ hidden|raw }}
</form>
<form action="{{cancel}}" method="post">
	<p><button type="submit">キャンセル</button></p>
</form>
