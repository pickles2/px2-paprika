{
    "name": "pickles2/px2-paprika",
    "description": "Publishing Lightweight Web Application with \"Pickles 2\".",
    "keywords": ["pickles2"],
    "license": "MIT",
    "scripts":{
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ],
        "documentation": [
            "php ./vendor/phpdocumentor/phpdocumentor/bin/phpdoc --title \"Paprika Framework 0.3.0-alpha.1+dev API Document\" -d \"./php/\" -t \"./docs/phpdoc/\""
        ]
    } ,
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "repositories": [
    ],
    "require": {
        "php": ">=5.4.0",
        "tomk79/request": "^1.0",
        "tomk79/filesystem": "^1.0"
    },
    "require-dev": {
        "pickles2/px-fw-2.x": "dev-develop",
        "pickles2/px2-multitheme": "2.0.*",
        "pickles2/px2-path-resolver": "2.0.*",
        "pickles2/px2-px2dthelper": "dev-develop",
        "pickles2/px2-remove-attr": "2.0.*",
        "pickles2/px2-sitemapexcel": "2.0.*",
        "pickles2/px2-publish-ex": "dev-develop",
        "phpunit/phpunit": "^4.1.0",
        "phpdocumentor/phpdocumentor": "^2.9.0",
        "jms/serializer": "~1.7"
    },
    "autoload": {
        "files": [
            "php/main.php",
            "php/fw/bowl.php",
            "php/fw/paprika.php"
        ]
    }
}
